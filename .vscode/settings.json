{
  /******************** vscode plugin - korofileheader ********************/
  "fileheader.customMade": { /**** 头部注释 ****/
    "custom_string_obkoro1": "Copyright 2020 姚嘉乐（Giannuo）\r\rLicensed under the Apache License, Version 2.0 (the \"License\");\ryou may not use this file except in compliance with the License.\rYou may obtain a copy of the License at\r\r    http://www.apache.org/licenses/LICENSE-2.0\r\rUnless required by applicable law or agreed to in writing, software\rdistributed under the License is distributed on an \"AS IS\" BASIS,\rWITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\rSee the License for the specific language governing permissions and\rlimitations under the License.\r",
    "Author": "Giannuo", //作者
    "Github": "https://github.com/Giannuo", //github
    "Date": "Do not edit", // 文件创建时间(不变)
    "LastEditTime": "Do not edit", // 文件最后编辑时间
    "FilePath": "Do not edit" // 文件在项目中的相对路径 自动更新
  },
  "fileheader.cursorMode": { /**** 函数注释 ****/},
  "fileheader.configObj": {
    "autoAdd": true, // 默认开启
    "prohibitAutoAdd": [
      "json",
      "yml",
      "md",
      "txt"
    ], // 禁止.json .md文件，自动添加头部注释
    "wideSame": true, // 设置为true开启
    "wideNum": 13 // 字段长度 默认为13
  },
  /******************** vscode plugin - i18n Ally 2.4.13 ********************/
  "i18n-ally.localesPaths": [
    "src/lang"
  ],
  "i18n-ally.enabledParsers": [
    "json",
    "yaml",
    "json5",
    "ts"
  ],
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.keystyle": "nested",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true, // 文件保存时，按照eslint规则格式化
  }
  /******************** ********************/
}
